<svg:svg xmlns:svg='http://www.w3.org/2000/svg'
	xmlns:xlink='http://www.w3.org/1999/xlink'
	xmlns='http://www.kite9.org/schema/adl'>

	<template id="divider" k9-shape="divider" k9-ui="orphan">
		<back />
	</template>
	
	<template id="methods"
		k9-ui="delete orphan cascade"
		k9-child="/public/examples/uml/uml-template.xml#method"
    k9-texture="none"
		k9-shape="rect">
		<back />
		<divider />

		<contents optional="true" />	
	</template>

	<template id="properties" 
		k9-ui="delete orphan cascade"
		k9-shape="rect"
		k9-texture="none"
		k9-child="/public/examples/uml/uml-template.xml#property">
	 
		<back />
		<divider />

		<contents optional="true" />		
	</template>
	
	<template id="class" k9-shape="rect"
		k9-ui="drag contain delete align connect autoconnect copy replace-connected cascade"
		k9-palette="connected"
		k9-texture="solid">
		<back k9-indicator="pulse" />
		<contents optional="true" />
	</template>
	
	<template id='entry' k9-ui="delete edit connect copy" >
      <contents optional="true" />
  	</template>
	
	<method id="method" k9-ui="edit orphan" k9-shape="rect">New Method</method>
	
	<property id="property" k9-ui="edit orphan" k9-shape="rect">New Property</property>
	

</svg:svg>