<svg:svg xmlns:svg='http://www.w3.org/2000/svg' xmlns='http://www.kite9.org/schema/adl' xmlns:xlink="http://www.w3.org/1999/xlink">

  <!-- tables -->

  <template id='table' k9-ui="drag delete layout" k9-palette="connected table">
    <back />
    <contents optional="true" />
  </template>
  
  <template id="cell" k9-ui="drag delete cascade orphan layout grid" k9-palette="cell" k9-contains="connected">
    <back k9-highlight="pulse stroke" />
    <contents optional="true" />
  </template>
  
  <template id="bordered-cell" k9-ui="drag delete cascade orphan layout grid" k9-palette="cell" k9-contains="connected">
    <back k9-highlight="pulse" />
    <contents optional="true" />
  </template>

  <cell id="cell-template"></cell>
  
  <cell id="cell-placeholder" class="temporary"></cell>

</svg:svg>
