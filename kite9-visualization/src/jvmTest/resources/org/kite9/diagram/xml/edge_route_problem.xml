<diagram  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	id="The Diagram">
	<context id="horiz" layout="VERTICAL" bordered="true">
		<glyph id="rob" stereotype="" label="Rob">
			<text-lines />
			<symbols />
		</glyph>
		<arrow id="met" label="met">
		</arrow>
		<glyph id="jenny" stereotype="" label="Jenny">
			<text-lines />
			<symbols />
		</glyph>
		<label xsi:type="text-line">
			<text>Vertical</text>
			<symbols />
		</label>
	</context>
	<arrow id="likes" label="likes">
	</arrow>
	<arrow id="eats" label="eats">
	</arrow>
	<context id="food" layout="VERTICAL" bordered="true">
		<glyph id="carrots" stereotype="" label="Carrots">
			<text-lines />
			<symbols />
		</glyph>
		<glyph id="chips" stereotype="" label="Chips">
			<text-lines />
			<symbols />
		</glyph>
		<glyph id="las" stereotype="" label="Lasagne">
			<text-lines />
			<symbols />
		</glyph>
		<label xsi:type="text-line">
			<text>Food</text>
			<symbols />
		</label>
	</context>

	
		<link id="id_5">
			<from xsi:type="arrow" reference="eats" />
			<to xsi:type="glyph" reference="rob" />
		</link>
		<link id="id_17">
			<from xsi:type="arrow" reference="met" />
			<to xsi:type="glyph" reference="rob" />
		</link>
		<link id="id_19">
			<from xsi:type="arrow" reference="met" />
			<to xsi:type="glyph" reference="jenny" />
		</link>
		<link id="id_21">
			<from xsi:type="glyph" reference="jenny" />
			<to xsi:type="arrow" reference="likes" />
		</link>
		<link id="id_14" >
			<from xsi:type="arrow" reference="likes" />
			<to xsi:type="glyph" reference="carrots" />
			<fromLabel xsi:type="text-line" id="tl6">
				<symbols />
			</fromLabel>
			<toLabel xsi:type="text-line" id="tl5">
				<symbols />
