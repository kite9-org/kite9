		<link id="id_9" >
			<from xsi:type="arrow" reference="eats" />
			<to xsi:type="context" reference="food" />
			<fromLabel xsi:type="text-line" id="tl1">
				<symbols />
			</fromLabel>
			<toLabel xsi:type="text-line" id="tl2">
